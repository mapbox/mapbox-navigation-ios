{"sections":[],"kind":"symbol","metadata":{"roleHeading":"Instance Property","role":"symbol","symbolKind":"property","modules":[{"name":"MapboxNavigationCore"}],"fragments":[{"text":"var","kind":"keyword"},{"kind":"text","text":" "},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"kind":"text","text":": "},{"preciseIdentifier":"s:Sb","kind":"typeIdentifier","text":"Bool"}],"externalID":"s:20MapboxNavigationCore0C6ConfigV012RouteRequestD0V21allowsUTurnAtWaypointSbvp","title":"allowsUTurnAtWaypoint"},"hierarchy":{"paths":[["doc:\/\/com.mapbox.navigation\/documentation\/Overview","doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore","doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig","doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct"]]},"identifier":{"url":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","interfaceLanguage":"swift"},"schemaVersion":{"patch":0,"minor":3,"major":0},"primaryContentSections":[{"declarations":[{"platforms":["iOS"],"languages":["swift"],"tokens":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","text":"Bool","kind":"typeIdentifier"}]}],"kind":"declarations"},{"content":[{"anchor":"discussion","level":2,"text":"Discussion","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"If the value of this property is ","type":"text"},{"type":"codeVoice","code":"true"},{"text":", a returned route may require an immediate U-turn at an","type":"text"},{"type":"text","text":" "},{"text":"intermediate","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"waypoint. At an intermediate waypoint, if the value of this property is "},{"code":"false","type":"codeVoice"},{"type":"text","text":", each returned route may"},{"type":"text","text":" "},{"type":"text","text":"continue straight ahead or turn to either side but may not U-turn. This property has no effect if only two"},{"type":"text","text":" "},{"text":"waypoints are specified.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Set this property to "},{"code":"true","type":"codeVoice"},{"text":" if you expect the user to traverse each leg of the trip separately. For","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"example, it"},{"text":" ","type":"text"},{"text":"would be quite easy for the user to effectively “U-turn” at a waypoint if the user first parks the car and","type":"text"},{"type":"text","text":" "},{"text":"patronizes a restaurant there before embarking on the next leg of the trip. Set this property to ","type":"text"},{"code":"false","type":"codeVoice"},{"text":" if","type":"text"},{"type":"text","text":" "},{"text":"you","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"expect the user to proceed to the next waypoint immediately upon arrival. For example, if the user only"},{"type":"text","text":" "},{"text":"needs to","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"drop off a passenger or package at the waypoint before continuing, it would be inconvenient to perform a"},{"type":"text","text":" "},{"type":"text","text":"U-turn"},{"type":"text","text":" "},{"type":"text","text":"at that location."},{"text":" ","type":"text"},{"text":"The default value of this property is `false.","type":"text"}]}],"kind":"content"}],"abstract":[{"text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate","type":"text"},{"type":"text","text":" "},{"text":"waypoint.","type":"text"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"]}],"references":{"doc://com.mapbox.navigation/documentation/MapboxNavigationCore":{"title":"MapboxNavigationCore","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore","url":"\/documentation\/mapboxnavigationcore","role":"collection","abstract":[{"type":"text","text":"The Navigation Core framework provides the basic components for the custom navigation experience."}],"kind":"symbol","type":"topic"},"doc://com.mapbox.navigation/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct":{"abstract":[{"text":"Mutable Routing configuration.","type":"text"}],"role":"symbol","kind":"symbol","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct","type":"topic","url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct","title":"CoreConfig.RouteRequestConfig","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"text":"RouteRequestConfig","kind":"identifier"}],"navigatorTitle":[{"text":"RouteRequestConfig","kind":"identifier"}]},"doc://com.mapbox.navigation/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct/allowsUTurnAtWaypoint":{"type":"topic","title":"allowsUTurnAtWaypoint","kind":"symbol","role":"symbol","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:Sb","text":"Bool"}],"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","abstract":[{"type":"text","text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate"},{"text":" ","type":"text"},{"type":"text","text":"waypoint."}],"url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"},"doc://com.mapbox.navigation/documentation/MapboxNavigationCore/CoreConfig":{"title":"CoreConfig","abstract":[{"text":"Mutable Core SDK configuration.","type":"text"}],"url":"\/documentation\/mapboxnavigationcore\/coreconfig","fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"CoreConfig","kind":"identifier"}],"type":"topic","kind":"symbol","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"CoreConfig"}]},"doc://com.mapbox.navigation/documentation/Overview":{"role":"collection","kind":"article","type":"topic","url":"\/documentation\/overview","title":"MapboxNavigation Documentation","abstract":[],"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/Overview"}}}