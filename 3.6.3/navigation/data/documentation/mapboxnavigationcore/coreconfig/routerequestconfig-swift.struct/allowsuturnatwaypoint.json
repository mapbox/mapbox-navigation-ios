":"keyword"},{"kind":"text","text":" "},{"text":"CoreConfig","kind":"identifier"}],"navigatorTitle":[{"text":"CoreConfig","kind":"identifier"}],"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxNavigationCore\/CoreConfig","url":"\/documentation\/mapboxnavigationcore\/coreconfig","role":"symbol","type":"topic","title":"CoreConfig","abstract":[{"type":"text","text":"Mutable Core SDK configuration."}],"kind":"symbol"}}}