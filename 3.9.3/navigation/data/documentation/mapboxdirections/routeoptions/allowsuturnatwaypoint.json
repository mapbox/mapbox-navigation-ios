t","text":"API."}]},"doc://com.mapbox.navigation/documentation/MapboxDirections/RouteOptions/allowsUTurnAtWaypoint":{"type":"topic","url":"\/documentation\/mapboxdirections\/routeoptions\/allowsuturnatwaypoint","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"text":": ","kind":"text"},{"text":"Bool","kind":"typeIdentifier","preciseIdentifier":"s:Sb"}],"kind":"symbol","title":"allowsUTurnAtWaypoint","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteOptions\/allowsUTurnAtWaypoint","role":"symbol","abstract":[{"text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate waypoint.","type":"text"}]},"doc://com.mapbox.navigation/documentation/Overview":{"url":"\/documentation\/overview","type":"topic","abstract":[],"role":"collection","title":"MapboxNavigation Documentation","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/Overview","kind":"article"},"doc://com.mapbox.navigation/documentation/MapboxDirections/ProfileIdentifier/automobileAvoidingTraffic":{"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/ProfileIdentifier\/automobileAvoidingTraffic","abstract":[{"type":"text","text":"The returned directions are appropriate for driving or riding a car, truck, or motorcycle."}],"kind":"symbol","role":"symbol","fragments":[{"text":"static","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"automobileAvoidingTraffic"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"ProfileIdentifier","preciseIdentifier":"s:16MapboxDirections17ProfileIdentifierV"}],"url":"\/documentation\/mapboxdirections\/profileidentifier\/automobileavoidingtraffic","title":"automobileAvoidingTraffic","type":"topic"}}}