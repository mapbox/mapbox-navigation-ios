{"identifier":{"url":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteOptions\/allowsUTurnAtWaypoint","interfaceLanguage":"swift"},"abstract":[{"text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate waypoint.","type":"text"}],"schemaVersion":{"minor":3,"major":0,"patch":0},"kind":"symbol","metadata":{"title":"allowsUTurnAtWaypoint","externalID":"s:16MapboxDirections12RouteOptionsC21allowsUTurnAtWaypointSbvp","symbolKind":"property","role":"symbol","modules":[{"name":"MapboxDirections"}],"roleHeading":"Instance Property","fragments":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:Sb","text":"Bool"}]},"hierarchy":{"paths":[["doc:\/\/com.mapbox.navigation\/documentation\/Overview","doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections","doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteOptions"]]},"primaryContentSections":[{"declarations":[{"languages":["swift"],"tokens":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","text":"Bool","kind":"typeIdentifier"}],"platforms":["iOS"]}],"kind":"declarations"},{"kind":"content","content":[{"level":2,"text":"Discussion","type":"heading","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"If the value of this property is ","type":"text"},{"type":"codeVoice","code":"true"},{"type":"text","text":", a returned route may require an immediate U-turn at an intermediate"},{"type":"text","text":" "},{"type":"text","text":"waypoint. At an intermediate waypoint, if the value of this property is "},{"code":"false","type":"codeVoice"},{"type":"text","text":", each returned route may"},{"type":"text","text":" "},{"type":"text","text":"continue straight ahead or turn 