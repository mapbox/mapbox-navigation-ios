":"symbol"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore":{"url":"\/documentation\/mapboxnavigationcore","kind":"symbol","role":"collection","abstract":[],"title":"MapboxNavigationCore","type":"topic","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore"}}}