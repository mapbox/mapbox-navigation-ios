umentation\/MapboxDirections","abstract":[{"text":"Quickly get driving, cycling, or walking directions, whether the trip is nonstop or it has multiple stops.","type":"text"}],"title":"MapboxDirections"},"doc://com.mapbox.navigation/documentation/MapboxDirections/RouteOptions":{"fragments":[{"kind":"keyword","text":"class"},{"text":" ","kind":"text"},{"kind":"identifier","text":"RouteOptions"}],"url":"\/documentation\/mapboxdirections\/routeoptions","kind":"symbol","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteOptions","navigatorTitle":[{"kind":"identifier","text":"RouteOptions"}],"type":"topic","role":"symbol","title":"RouteOptions","abstract":[{"type":"text","text":"A "},{"type":"reference","isActive":true,"identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/RouteOptions"},{"text":" object is a structure that specifies the criteria for results returned by the Mapbox Directions","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"API."}]},"doc://com.mapbox.navigation/documentation/MapboxDirections/ProfileIdentifier/automobileAvoidingTraffic":{"role":"symbol","type":"topic","url":"\/documentation\/mapboxdirections\/profileidentifier\/automobileavoidingtraffic","abstract":[{"text":"The returned directions are appropriate for driving or riding a car, truck, or motorcycle.","type":"text"}],"kind":"symbol","identifier":"doc:\/\/com.mapbox.navigation\/documentation\/MapboxDirections\/ProfileIdentifier\/automobileAvoidingTraffic","title":"automobileAvoidingTraffic","fragments":[{"text":"static","kind":"keyword"},{"text":" ","kind":"text"},{"text":"let","kind":"keyword"},{"kind":"text","text":" "},{"text":"automobileAvoidingTraffic","kind":"identifier"},{"kind":"text","text":": "},{"text":"ProfileIdentifier","preciseIdentifier":"s:16MapboxDirections17ProfileIdentifierV","kind":"typeIdentifier"}]}}}