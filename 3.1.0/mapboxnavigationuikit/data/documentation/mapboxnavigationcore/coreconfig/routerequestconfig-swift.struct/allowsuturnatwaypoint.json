{"hierarchy":{"paths":[["doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct"]]},"kind":"symbol","metadata":{"roleHeading":"Instance Property","role":"symbol","symbolKind":"property","modules":[{"name":"MapboxNavigationCore"}],"externalID":"s:20MapboxNavigationCore0C6ConfigV012RouteRequestD0V21allowsUTurnAtWaypointSbvp","title":"allowsUTurnAtWaypoint","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"kind":"text","text":": "},{"preciseIdentifier":"s:Sb","kind":"typeIdentifier","text":"Bool"}]},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"]}],"primaryContentSections":[{"declarations":[{"tokens":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","text":"Bool","kind":"typeIdentifier"}],"languages":["swift"],"platforms":["iOS"]}],"kind":"declarations"},{"kind":"content","content":[{"anchor":"discussion","type":"heading","text":"Discussion","level":2},{"inlineContent":[{"type":"text","text":"If the value of this property is "},{"type":"codeVoice","code":"true"},{"text":", a returned route may require an immediate U-turn at an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"intermediate"},{"text":" ","type":"text"},{"type":"text","text":"waypoint. At an intermediate waypoint, if the value of this property is "},{"code":"false","type":"codeVoice"},{"type":"text","text":", each returned route may"},{"type":"text","text":" "},{"text":"continue straight ahead or turn to either side but may not U-turn. This property has no effect if only two","type":"text"},{"type":"text","text":" "},{"type":"text","text":"waypoints are specified."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"text":"Set this property to ","type":"text"},{"type":"codeVoice","code":"true"},{"text":" if you expect the user to traverse each leg of the trip separately. For","type":"text"},{"type":"text","text":" "},{"type":"text","text":"example, it"},{"text":" ","type":"text"},{"text":"would be quite easy for the user to effectively “U-turn” at a waypoint if the user first parks the car and","type":"text"},{"text":" ","type":"text"},{"text":"patronizes a restaurant there before embarking on the next leg of the trip. Set this property to ","type":"text"},{"code":"false","type":"codeVoice"},{"type":"text","text":" if"},{"text":" ","type":"text"},{"text":"you","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"expect the user to proceed to the next waypoint immediately upon arrival. For example, if the user only"},{"type":"text","text":" "},{"text":"needs to","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"drop off a passenger or package at the waypoint before continuing, it would be inconvenient to perform a"},{"type":"text","text":" "},{"type":"text","text":"U-turn"},{"text":" ","type":"text"},{"text":"at that location.","type":"text"},{"text":" ","type":"text"},{"text":"The default value of this property is `false.","type":"text"}]}]}],"abstract":[{"text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate","type":"text"},{"type":"text","text":" "},{"type":"text","text":"waypoint."}],"sections":[],"schemaVersion":{"patch":0,"minor":3,"major":0},"identifier":{"url":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","interfaceLanguage":"swift"},"references":{"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct/