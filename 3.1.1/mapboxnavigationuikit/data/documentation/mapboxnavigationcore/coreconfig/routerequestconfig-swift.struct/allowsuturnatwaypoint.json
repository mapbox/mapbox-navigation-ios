{"hierarchy":{"paths":[["doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct"]]},"schemaVersion":{"major":0,"patch":0,"minor":3},"abstract":[{"text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate","type":"text"},{"type":"text","text":" "},{"type":"text","text":"waypoint."}],"sections":[],"identifier":{"url":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","interfaceLanguage":"swift"},"kind":"symbol","primaryContentSections":[{"declarations":[{"languages":["swift"],"platforms":["iOS"],"tokens":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","kind":"typeIdentifier","text":"Bool"}]}],"kind":"declarations"},{"kind":"content","content":[{"type":"heading","level":2,"text":"Discussion","anchor":"discussion"},{"inlineContent":[{"type":"text","text":"If the value of this property is "},{"code":"true","type":"codeVoice"},{"type":"text","text":", a returned route may require an immediate U-turn at an"},{"type":"text","text":" "},{"type":"text","text":"intermediate"},{"type":"text","text":" "},{"type":"text","text":"waypoint. At an intermediate waypoint, if the value of this property is "},{"type":"codeVoice","code":"false"},{"type":"text","text":", each returned route may"},{"text":" ","type":"text"},{"type":"text","text":"continue straight ahead or turn to either side but may not U-turn. This property has no effect if only two"},{"type":"text","text":" "},{"type":"text","text":"waypoints are specified."}],"type":"paragraph"},{"inlineContent":[{"text":"Set this property to ","type":"text"},{"type":"codeVoice","code":"true"},{"type":"text","text":" if you expect the user to traverse each leg of the trip separately. For"},{"text":" ","type":"text"},{"type":"text","text":"example, it"},{"text":" ","type":"text"},{"text":"would be quite easy for the user to effectively “U-turn” at a waypoint if the user first parks the car and","type":"text"},{"type":"text","text":" "},{"text":"patronizes a restaurant there before embarking on the next leg of the trip. Set this property to ","type":"text"},{"type":"codeVoice","code":"false"},{"type":"text","text":" if"},{"text":" ","type":"text"},{"text":"you","type":"text"},{"type":"text","text":" "},{"type":"text","text":"expect the user to proceed to the next waypoint immediately upon arrival. For example, if the user only"},{"text":" ","type":"text"},{"type":"text","text":"needs to"},{"text":" ","type":"text"},{"type":"text","text":"drop off a passenger or package at the waypoint before continuing, it would be inconvenient to perform a"},{"type":"text","text":" "},{"type":"text","text":"U-turn"},{"type":"text","text":" "},{"type":"text","text":"at that location."},{"type":"text","text":" "},{"type":"text","text":"The default value of this property is `false."}],"type":"paragraph"}]}],"metadata":{"roleHeading":"Instance Property","role":"symbol","title":"allowsUTurnAtWaypoint","symbolKind":"property","externalID":"s:20MapboxNavigationCore0C6ConfigV012RouteRequestD0V21allowsUTurnAtWaypointSbvp","modules":[{"name":"MapboxNavigationCore"}],"fragments":[{"kind":"keyword","text":"var"},{"text":" ","kind":"text"},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"}]},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"]}],"references":{"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct":{"role":"symbol","title":"CoreConfig.RouteRequestConfig","url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct","navigatorTitle":[{"text":"RouteRequestConfig","kind":"identifier"}],"abstract":[{"text":"Mutable Routing configuration.","type":"text"}],"kind":"symbol","fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"text":"RouteRequestConfig","kind":"identifier"}],"identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct","type":"topic"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct/allowsUTurnAtWaypoint":{"title":"allowsUTurnAtWaypoint","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","text":"Bool","kind":"typeIdentifier"}],"role":"symbol","url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint","kind":"symbol","abstract":[{"type":"text","text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate"},{"text":" ","type":"text"},{"text":"waypoint.","type":"text"}],"type":"topic"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig":{"fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"kind":"identifier","text":"CoreConfig"}],"type":"topic","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","url":"\/documentation\/mapboxnavigationcore\/coreconfig","title":"CoreConfig","abstract":[{"text":"Mutable Core SDK configuration.","type":"text"}],"kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"CoreConfig"}],"role":"symbol"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore":{"title":"MapboxNavigationCore","type":"topic","url":"\/documentation\/mapboxnavigationcore","abstract":[],"identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","role":"collection","kind":"symbol"}}}