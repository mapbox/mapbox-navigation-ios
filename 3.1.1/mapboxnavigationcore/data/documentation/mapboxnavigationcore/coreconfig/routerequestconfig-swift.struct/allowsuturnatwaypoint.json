{"identifier":{"url":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","interfaceLanguage":"swift"},"abstract":[{"text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate","type":"text"},{"text":" ","type":"text"},{"text":"waypoint.","type":"text"}],"metadata":{"modules":[{"name":"MapboxNavigationCore"}],"fragments":[{"text":"var","kind":"keyword"},{"kind":"text","text":" "},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:Sb","text":"Bool","kind":"typeIdentifier"}],"roleHeading":"Instance Property","externalID":"s:20MapboxNavigationCore0C6ConfigV012RouteRequestD0V21allowsUTurnAtWaypointSbvp","title":"allowsUTurnAtWaypoint","symbolKind":"property","role":"symbol"},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"allowsUTurnAtWaypoint"},{"text":": ","kind":"text"},{"text":"Bool","kind":"typeIdentifier","preciseIdentifier":"s:Sb"}],"platforms":["iOS"],"languages":["swift"]}]},{"kind":"content","content":[{"level":2,"text":"Discussion","type":"heading","anchor":"discussion"},{"type":"paragraph","inlineContent":[{"text":"If the value of this property is ","type":"text"},{"code":"true","type":"codeVoice"},{"type":"text","text":", a returned route may require an immediate U-turn at an"},{"type":"text","text":" "},{"type":"text","text":"intermediate"},{"type":"text","text":" "},{"text":"waypoint. At an intermediate waypoint, if the value of this property is ","type":"text"},{"code":"false","type":"codeVoice"},{"type":"text","text":", each returned route may"},{"type":"text","text":" "},{"text":"continue straight ahead or turn to either side but may not U-turn. This property has no effect if only two","type":"text"},{"type":"text","text":" "},{"type":"text","text":"waypoints are specified."}]},{"type":"paragraph","inlineContent":[{"text":"Set this property to ","type":"text"},{"code":"true","type":"codeVoice"},{"text":" if you expect the user to traverse each leg of the trip separately. For","type":"text"},{"type":"text","text":" "},{"type":"text","text":"example, it"},{"type":"text","text":" "},{"type":"text","text":"would be quite easy for the user to effectively “U-turn” at a waypoint if the user first parks the car and"},{"type":"text","text":" "},{"type":"text","text":"patronizes a restaurant there before embarking on the next leg of the trip. Set this property to "},{"type":"codeVoice","code":"false"},{"type":"text","text":" if"},{"text":" ","type":"text"},{"type":"text","text":"you"},{"text":" ","type":"text"},{"type":"text","text":"expect the user to proceed to the next waypoint immediately upon arrival. For example, if the user only"},{"text":" ","type":"text"},{"type":"text","text":"needs to"},{"text":" ","type":"text"},{"text":"drop off a passenger or package at the waypoint before continuing, it would be inconvenient to perform a","type":"text"},{"type":"text","text":" "},{"type":"text","text":"U-turn"},{"type":"text","text":" "},{"type":"text","text":"at that location."},{"type":"text","text":" "},{"type":"text","text":"The default value of this property is `false."}]}]}],"hierarchy":{"paths":[["doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig","doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct"]]},"sections":[],"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"symbol","variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint"]}],"references":{"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig":{"type":"topic","navigatorTitle":[{"text":"CoreConfig","kind":"identifier"}],"url":"\/documentation\/mapboxnavigationcore\/coreconfig","kind":"symbol","fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"text":"CoreConfig","kind":"identifier"}],"title":"CoreConfig","abstract":[{"type":"text","text":"Mutable Core SDK configuration."}],"role":"symbol","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct/allowsUTurnAtWaypoint":{"url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct\/allowsuturnatwaypoint","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct\/allowsUTurnAtWaypoint","abstract":[{"type":"text","text":"A Boolean value that indicates whether a returned route may require a point U-turn at an intermediate"},{"type":"text","text":" "},{"type":"text","text":"waypoint."}],"type":"topic","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"text":"allowsUTurnAtWaypoint","kind":"identifier"},{"kind":"text","text":": "},{"kind":"typeIdentifier","preciseIdentifier":"s:Sb","text":"Bool"}],"title":"allowsUTurnAtWaypoint","kind":"symbol","role":"symbol"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore":{"role":"collection","url":"\/documentation\/mapboxnavigationcore","abstract":[],"type":"topic","identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore","title":"MapboxNavigationCore","kind":"symbol"},"doc://MapboxNavigationCore/documentation/MapboxNavigationCore/CoreConfig/RouteRequestConfig-swift.struct":{"title":"CoreConfig.RouteRequestConfig","url":"\/documentation\/mapboxnavigationcore\/coreconfig\/routerequestconfig-swift.struct","role":"symbol","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"RouteRequestConfig"}],"abstract":[{"type":"text","text":"Mutable Routing configuration."}],"fragments":[{"kind":"keyword","text":"struct"},{"text":" ","kind":"text"},{"kind":"identifier","text":"RouteRequestConfig"}],"identifier":"doc:\/\/MapboxNavigationCore\/documentation\/MapboxNavigationCore\/CoreConfig\/RouteRequestConfig-swift.struct","type":"topic"}}}